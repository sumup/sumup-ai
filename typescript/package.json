{
  "name": "@sumup/agent-toolkit",
  "version": "0.8.0",
  "homepage": "https://github.com/sumup/sumup-ai",
  "license": "Apache-2.0",
  "author": "SumUp <support@sumup.com> (https://sumup.com/)",
  "type": "module",
  "scripts": {
    "build": "rslib build",
    "build:watch": "rslib build --watch",
    "inspect": "rslib inspect",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/sumup-ai.git"
  },
  "exports": {
    "./langchain": {
      "types": "./dist/langchain/index.d.ts",
      "require": "./dist/langchain/index.cjs",
      "import": "./dist/langchain/index.mjs"
    },
    "./openai": {
      "types": "./dist/openai/index.d.ts",
      "require": "./dist/openai/index.cjs",
      "import": "./dist/openai/index.mjs"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "require": "./dist/mcp/index.cjs",
      "import": "./dist/mcp/index.mjs"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "require": "./dist/ai/index.cjs",
      "import": "./dist/ai/index.mjs"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@langchain/core": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@openai/agents": "^0.3.3",
    "@sumup/sdk": "^0.0.11",
    "ai": "^5.0.103",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@rslib/core": "^0.18.2",
    "@types/node": "^24.10.1",
    "rsbuild-plugin-publint": "^0.3.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "dist/**/*"
  ]
}
